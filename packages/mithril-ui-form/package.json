{
  "name": "mithril-ui-form",
  "version": "0.1.2",
  "description": "Convert a JSON file to a dynamic, materialized form.",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvullings/mithril-ui-form.git"
  },
  "scripts": {
    "link": "npm link",
    "clean": "rimraf .rpt2_cache lib dist",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "dry-run": "npm publish --dry-run",
    "patch-release": "npm run clean && npm run build && npm version patch --force -m \"Patch release\" && npm publish && git push --follow-tags",
    "minor-release": "npm run clean && npm run build && npm version minor --force -m \"Minor release\" && npm publish && git push --follow-tags",
    "major-release": "npm run clean && npm run build && npm version major --force -m \"Major release\" && npm publish && git push --follow-tags",
    "gen:schema": "typescript-json-schema.cmd tsconfig.json Form > ./schema/ui-form.json"
  },
  "keywords": [
    "mithril",
    "dynamic",
    "json",
    "form",
    "materialize"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "dependencies": {
    "slimdown-js": "^0.1.4"
  },
  "devDependencies": {
    "@types/leaflet": "^1.4.6",
    "@types/leaflet-draw": "^0.4.14",
    "@types/materialize-css": "^1.0.6",
    "@types/mithril": "github:MithrilJS/mithril.d.ts#v2",
    "@types/geojson": "^7946.0.7",
    "mithril-materialized": "^0.13.15",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "typescript-json-schema": "^0.38.3"
  },
  "peerDependencies": {
    "mithril": "^2.0.0-rc.7",
    "mithril-materialized": "^0.13.15",
    "mithril-leaflet": "0.1.5",
    "materialize-css": "^1.0.0",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4"
  }
}
