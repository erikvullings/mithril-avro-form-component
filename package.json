{
  "name": "mithril-ui-form-project",
  "private": true,
  "version": "0.0.0",
  "description": "Based on a JSON document, generate a UI form for the Mithril framework.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvullings/mithril-tree-component.git"
  },
  "keywords": [
    "mithril",
    "form",
    "component",
    "typescript"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/mithril-tree-component/issues"
  },
  "homepage": "https://github.com/erikvullings/mithril-tree-component#readme",
  "scripts": {
    "postinstall": "npm run link && npm run build",
    "link": "lerna run link --parallel",
    "build": "lerna run build --parallel",
    "clean:local": "rimraf -fr docs",
    "clean": "npm run clean:local && lerna run --parallel clean",
    "start": "lerna run --parallel start",
    "patch-release": "lerna run patch-release --parallel",
    "minor-release": "lerna run minor-release --parallel"
  },
  "devDependencies": {
    "lerna": "^3.15.0",
    "rimraf": "^2.6.3"
  }
}
